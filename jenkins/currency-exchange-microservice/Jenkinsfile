// sample pipeline
pipeline {
    //agent {any{ image 'maven:3.8.3-openjdk-17'}}
	agent any
	environment {
		mavenHome = tool 'MAVEN'
		PATH = "$mavenHome/bin:$PATH"
	}
    stages {
        stage ('checkout') {
            steps {
				echo "mvn --version"
                sh 'mvn --version'
			}
        }
        stage ('Build') {
            steps {
				sh 'mvn -f .'
				sh 'mvn clean compile'
			}
        }
		stage('Test'){
			steps {
				sh 'mvn test'
			}
		}
    }
}
